{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "import/extensions": 0,
    "no-console": 0,
    "operator-linebreak": 0,
    "linebreak-style":0,
    "comma-dangle":0
  }
}
